<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="net.edu.module.dao.ArchiveTestScoreDao">
    <insert id="insertArchiveTestScore" parameterType="java.util.List">
        replace into archive_test_score
        (stu_id,stu_name,test_id,score,test_id) values
        <foreach collection="list" item="item"  separator=",">
            (#{stuId},#{stuName},#{item.score},1)
        </foreach>
    </insert>
</mapper>
