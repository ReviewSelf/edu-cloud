<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="net.edu.module.dao.TeacherClassRecordDao">

    <insert id="insertClassUserRecord" >
        insert into teach_class_record(type,record_time,students_id,teacher_id,old_class_id,old_lesson_id,new_class_id,new_lesson_id)
            values(#{vo.type},#{vo.time},#{vo.studentId},#{vo.teacherId},#{vo.oldClassId},#{vo.oldLessonId},#{vo.newClassId},#{vo.newLessonId})
    </insert>

</mapper>
