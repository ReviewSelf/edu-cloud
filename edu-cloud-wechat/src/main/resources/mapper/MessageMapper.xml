<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="net.edu.module.dao.MessageDao">
    <select id="selectUserOpenIdById" resultType="java.lang.String">
        select open_id as openId from sys_user where id = #{id}
